{
  "name": "odds-collector",
  "version": "0.1.0",
  "description": "Cloudflare Worker for automated football odds collection with event-based scheduling, D1 persistence, and R2 storage",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "tail": "wrangler tail",
    "test": "jest",
    "lint": "eslint src **/**.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "backfill": "ts-node src/scripts/backfill.ts",
    "rebuild-indexes": "ts-node src/scripts/rebuildIndexes.ts"
  },
  "keywords": [
    "sports",
    "betting",
    "odds",
    "api",
    "collection",
    "automation",
    "scheduling",
    "cloudflare",
    "workers",
    "r2",
    "d1",
    "football",
    "soccer"
  ],
  "author": "Benjamin Bours",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benjaminbours/odds-collector.git"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251113.0",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "globals": "^16.5.0",
    "jest": "^29.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.46.3",
    "wrangler": "^4.46.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.0.0",
    "@footdata/shared": "file:../footdata/typescript/shared"
  }
}
